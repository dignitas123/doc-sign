    "openpgp": "^5.3.1",
    "prettier": "^2.7.1",
    "quasar": "^2.7.5",
    "vue": "^3.2.37",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0",
    "vuex": "^4.0.1",
    "web3.storage": "^3.5.3"
