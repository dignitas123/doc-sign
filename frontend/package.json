  "description": "Sign and store documents safely with smart contracts on the blockchain with DocSign.",
  "productName": "DocSign",
  "author": "Niclas Hummel <niclashummel.fx@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "dev": "quasar dev",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
